-- Scripts:

local function OMIZF_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	_G.Nitradenag = false
	local speaker = game.Players.LocalPlayer
	local localspspeed = 16       ---- asta schimbam sus la cod da?
	function isNumber(str)
		if tonumber(str) ~= nil or str == 'inf' then
			return true
		end
	end
	local RunService = game:GetService("RunService")
	local HumanModCons = {}
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitradenag == false then
			_G.Nitradenag = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			if isNumber(localspspeed) then
				local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
				local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")
				local function WalkSpeedChange()
					if Char and Human and _G.Nitradenag then
						Human.WalkSpeed = localspspeed
					end
				end
				RunService.Heartbeat:Connect(function()
					WalkSpeedChange()
				end)
				HumanModCons.wsLoop = (HumanModCons.wsLoop and HumanModCons.wsLoop:Disconnect() and false) or Human:GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
				HumanModCons.wsCA = (HumanModCons.wsCA and HumanModCons.wsCA:Disconnect() and false) or speaker.CharacterAdded:Connect(function(nChar)
					Char, Human = nChar, nChar:WaitForChild("Humanoid")
					WalkSpeedChange()
					HumanModCons.wsLoop = (HumanModCons.wsLoop and HumanModCons.wsLoop:Disconnect() and false) or Human:GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
				end)
			end
		else
			_G.Nitradenag = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			HumanModCons.ajLoop = (HumanModCons.ajLoop and HumanModCons.ajLoop:Disconnect() and false) or nil
			HumanModCons.ajCA = (HumanModCons.ajCA and HumanModCons.ajCA:Disconnect() and false) or nil
		end
	end)
end
coroutine.wrap(OMIZF_fake_script)()
local function AZHFKI_fake_script() -- localspds.LocalScript 
	local script = Instance.new('LocalScript', localspds)

	
	local slider = {}
	
	function slider:New(shared)
		local dragging = false
		local uis = game:GetService("UserInputService")
	
		shared.Default = math.clamp(shared.Default,shared.Range[1],shared.Range[2])
	
		local domain = script.Parent
		local progress = domain.Progress
		local interact = domain.Interact
		local value = domain.Value
	
		function shared:set(newValue)
			shared.Callback(shared.Default)
			newValue = math.clamp(newValue,shared.Range[1],shared.Range[2])
			if shared.Suffix then
				value.Text = tostring(newValue) .. " " .. shared.Suffix
			else
				value.Text = tostring(newValue)
			end
			local size = domain.AbsoluteSize.X * (shared.Default - shared.Range[1]) / (shared.Range[2] - shared.Range[1])
			if size < 5 then
				size = 5
			elseif size > (domain.AbsolutePosition.X + domain.AbsoluteSize.X) then
				size = domain.AbsolutePosition.X + domain.AbsoluteSize.X
			end
			game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size=UDim2.new(0,size,1,0)}):Play()
		end
	
		if shared.Suffix then
			value.Text = tostring(shared.Default) .. " " .. shared.Suffix
		else
			value.Text = tostring(shared.Default)
		end
	
		interact.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
			end
		end)
		interact.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false
			end
		end)
	
		interact.MouseButton1Down:Connect(function(x)
			local location = x
			local ren ren = game:GetService("RunService").Stepped:Connect(function()
				if dragging then
					location = uis:GetMouseLocation().X
					if location < domain.AbsolutePosition.X then
						location = domain.AbsolutePosition.X
					elseif location > domain.AbsolutePosition.X + domain.AbsoluteSize.X then
						location = domain.AbsolutePosition.X + domain.AbsoluteSize.X
					end
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.45,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X,1,0)}):Play()
					local new = shared.Range[1] + (location - domain.AbsolutePosition.X) / domain.AbsoluteSize.X * (shared.Range[2] - shared.Range[1])
					new = math.floor(new / shared.Increment + .5) * (shared.Increment * 10000000) / 10000000
					if shared.Suffix then
						value.Text = tostring(new) .. " " .. shared.Suffix
					else
						value.Text = tostring(new)
					end
					shared.Callback(new)
				else
					ren:Disconnect()
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X > 5 and location - domain.AbsolutePosition.X or 5,1,0)}):Play()
				end
			end)
		end)
	end
	slider:New({
		Range={16,24},
		Increment=1,
		Suffix="/24",
		Default=0,
		Callback=function(sliderudelocalspeed)
			localspspeed = sliderudelocalspeed
			end
	})
	
end
coroutine.wrap(AZHFKI_fake_script)()
local function IEHRFH_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	_G.Nitraden = false
	local workspace = game:GetService("Workspace")
	local runService = game:GetService("RunService")
	local fieldOfViewEnabled = false -- Toggle variable
	local viewfovlocalplayer = 65   --------asta sus la scripturi da?
	local defaultFieldOfView = 65
	local function setFieldOfView()
		if fieldOfViewEnabled then
			workspace.CurrentCamera.FieldOfView = viewfovlocalplayer
		else
			workspace.CurrentCamera.FieldOfView = defaultFieldOfView
		end
	end
	runService.Heartbeat:Connect(function()
		setFieldOfView()
	end)
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitraden == false then
			_G.Nitraden = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			fieldOfViewEnabled = true
		else
			_G.Nitraden = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			fieldOfViewEnabled = false
		end
	end)
end
coroutine.wrap(IEHRFH_fake_script)()
local function FDGSDU_fake_script() -- localfo.LocalScript 
	local script = Instance.new('LocalScript', localfo)

	
	local slider = {}
	
	function slider:New(shared)
		local dragging = false
		local uis = game:GetService("UserInputService")
	
		shared.Default = math.clamp(shared.Default,shared.Range[1],shared.Range[2])
	
		local domain = script.Parent
		local progress = domain.Progress
		local interact = domain.Interact
		local value = domain.Value
	
		function shared:set(newValue)
			shared.Callback(shared.Default)
			newValue = math.clamp(newValue,shared.Range[1],shared.Range[2])
			if shared.Suffix then
				value.Text = tostring(newValue) .. " " .. shared.Suffix
			else
				value.Text = tostring(newValue)
			end
			local size = domain.AbsoluteSize.X * (shared.Default - shared.Range[1]) / (shared.Range[2] - shared.Range[1])
			if size < 5 then
				size = 5
			elseif size > (domain.AbsolutePosition.X + domain.AbsoluteSize.X) then
				size = domain.AbsolutePosition.X + domain.AbsoluteSize.X
			end
			game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size=UDim2.new(0,size,1,0)}):Play()
		end
	
		if shared.Suffix then
			value.Text = tostring(shared.Default) .. " " .. shared.Suffix
		else
			value.Text = tostring(shared.Default)
		end
	
		interact.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
			end
		end)
		interact.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false
			end
		end)
	
		interact.MouseButton1Down:Connect(function(x)
			local location = x
			local ren ren = game:GetService("RunService").Stepped:Connect(function()
				if dragging then
					location = uis:GetMouseLocation().X
					if location < domain.AbsolutePosition.X then
						location = domain.AbsolutePosition.X
					elseif location > domain.AbsolutePosition.X + domain.AbsoluteSize.X then
						location = domain.AbsolutePosition.X + domain.AbsoluteSize.X
					end
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.45,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X,1,0)}):Play()
					local new = shared.Range[1] + (location - domain.AbsolutePosition.X) / domain.AbsoluteSize.X * (shared.Range[2] - shared.Range[1])
					new = math.floor(new / shared.Increment + .5) * (shared.Increment * 10000000) / 10000000
					if shared.Suffix then
						value.Text = tostring(new) .. " " .. shared.Suffix
					else
						value.Text = tostring(new)
					end
					shared.Callback(new)
				else
					ren:Disconnect()
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X > 5 and location - domain.AbsolutePosition.X or 5,1,0)}):Play()
				end
			end)
		end)
	end
	slider:New({
		Range={65,120},
		Increment=1,
		Suffix="/120",
		Default=65,
		Callback=function(sliderumasiidrq)
			viewfovlocalplayer = sliderumasiidrq
		end
	})
	
end
coroutine.wrap(FDGSDU_fake_script)()
local function DYXD_fake_script() -- TextButton_8.LocalScript 
	local script = Instance.new('LocalScript', TextButton_8)

	_G.Nitradenaac = false
	local RunService = game:GetService("RunService")
	local playa = game.Players.LocalPlayer
	local Noclipping = nil
	local Clip = true
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitradenaac == false then
			_G.Nitradenaac = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			Clip = false
			wait(0.1)
			local function NoclipLoop()
				if Clip == false and playa.Character ~= nil then
					for _, child in pairs(playa.Character:GetDescendants()) do
						if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= floatName then
							child.CanCollide = false
						end
					end
				end
			end
			Noclipping = RunService.Stepped:Connect(NoclipLoop)
		else
			_G.Nitradenaac = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			if Noclipping then
				Noclipping:Disconnect()
			end
			playa.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
			Clip = true
		end
	end)
end
coroutine.wrap(DYXD_fake_script)()
local function IMGPNDP_fake_script() -- TextButton_9.LocalScript 
	local script = Instance.new('LocalScript', TextButton_9)

	_G.Nitradenag = false
	local speaker = game.Players.LocalPlayer
	local localspspeed = 23       ---- asta schimbam sus la cod da?
	function isNumber(str)
		if tonumber(str) ~= nil or str == 'inf' then
			return true
		end
	end
	local RunService = game:GetService("RunService")
	local HumanModCons = {}
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitradenag == false then
			_G.Nitradenag = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			if isNumber(localspspeed) then
				local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
				local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")
				local function WalkSpeedChange()
					if Char and Human and _G.Nitradenag then
						Human.WalkSpeed = localspspeed
					end
				end
				RunService.Heartbeat:Connect(function()
					WalkSpeedChange()
				end)
				HumanModCons.wsLoop = (HumanModCons.wsLoop and HumanModCons.wsLoop:Disconnect() and false) or Human:GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
				HumanModCons.wsCA = (HumanModCons.wsCA and HumanModCons.wsCA:Disconnect() and false) or speaker.CharacterAdded:Connect(function(nChar)
					Char, Human = nChar, nChar:WaitForChild("Humanoid")
					WalkSpeedChange()
					HumanModCons.wsLoop = (HumanModCons.wsLoop and HumanModCons.wsLoop:Disconnect() and false) or Human:GetPropertyChangedSignal("WalkSpeed"):Connect(WalkSpeedChange)
				end)
			end
		else
			_G.Nitradenag = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			HumanModCons.ajLoop = (HumanModCons.ajLoop and HumanModCons.ajLoop:Disconnect() and false) or nil
			HumanModCons.ajCA = (HumanModCons.ajCA and HumanModCons.ajCA:Disconnect() and false) or nil
		end
	end)
end
