local function JQMY_fake_script() -- dropdwnpr.LocalScript 
	local script = Instance.new('LocalScript', dropdwnpr)

	local drop = script.Parent
	local menu = drop.Menu
	local selection = drop.select
	local openx = false
	selection.MouseButton1Click:Connect(function()
		if not openx then
			menu:TweenSize(UDim2.new(1, 0, 6, 0), "Out", "Sine", 0.2, true)
			for _, button in pairs(menu:GetChildren()) do
				if button:IsA("TextButton") then
					button.Visible = true
					menu.Visible = true
				end
			end
			openx = true
		else
			menu:TweenSize(UDim2.new(1, 0, 0, 0), "Out", "Sine", 0.3, true)
			wait(0.055)
			for _, button in pairs(menu:GetChildren()) do
				if button:IsA("TextButton") then
					button.Visible = false
				end
			end
			openx = false
		end
	end)
	for _, button in pairs(menu:GetChildren()) do
		if button:IsA("TextButton") then
			button.MouseEnter:Connect(function()
				button.BackgroundTransparency = 0.8
			end)
			button.MouseLeave:Connect(function()
				button.BackgroundTransparency = 1
			end)
		end
	end
end
coroutine.wrap(JQMY_fake_script)()
local function ZULY_fake_script() -- Domain.LocalScript 
	local script = Instance.new('LocalScript', Domain)

	
	local slider = {}
	
	function slider:New(shared)
		local dragging = false
		local uis = game:GetService("UserInputService")
	
		shared.Default = math.clamp(shared.Default,shared.Range[1],shared.Range[2])
	
		local domain = script.Parent
		local progress = domain.Progress
		local interact = domain.Interact
		local value = domain.Value
	
		function shared:set(newValue)
			shared.Callback(shared.Default)
			newValue = math.clamp(newValue,shared.Range[1],shared.Range[2])
			if shared.Suffix then
				value.Text = tostring(newValue) .. " " .. shared.Suffix
			else
				value.Text = tostring(newValue)
			end
			local size = domain.AbsoluteSize.X * (shared.Default - shared.Range[1]) / (shared.Range[2] - shared.Range[1])
			if size < 5 then
				size = 5
			elseif size > (domain.AbsolutePosition.X + domain.AbsoluteSize.X) then
				size = domain.AbsolutePosition.X + domain.AbsoluteSize.X
			end
			game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size=UDim2.new(0,size,1,0)}):Play()
		end
	
		if shared.Suffix then
			value.Text = tostring(shared.Default) .. " " .. shared.Suffix
		else
			value.Text = tostring(shared.Default)
		end
	
		interact.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
			end
		end)
		interact.InputEnded:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = false
			end
		end)
	
		interact.MouseButton1Down:Connect(function(x)
			local location = x
			local ren ren = game:GetService("RunService").Stepped:Connect(function()
				if dragging then
					location = uis:GetMouseLocation().X
					if location < domain.AbsolutePosition.X then
						location = domain.AbsolutePosition.X
					elseif location > domain.AbsolutePosition.X + domain.AbsoluteSize.X then
						location = domain.AbsolutePosition.X + domain.AbsoluteSize.X
					end
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.45,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X,1,0)}):Play()
					local new = shared.Range[1] + (location - domain.AbsolutePosition.X) / domain.AbsoluteSize.X * (shared.Range[2] - shared.Range[1])
					new = math.floor(new / shared.Increment + .5) * (shared.Increment * 10000000) / 10000000
					if shared.Suffix then
						value.Text = tostring(new) .. " " .. shared.Suffix
					else
						value.Text = tostring(new)
					end
					shared.Callback(new)
				else
					ren:Disconnect()
					game:GetService("TweenService"):Create(progress,TweenInfo.new(.3,Enum.EasingStyle.Quint,Enum.EasingDirection.Out),{Size = UDim2.new(0,location - domain.AbsolutePosition.X > 5 and location - domain.AbsolutePosition.X or 5,1,0)}):Play()
				end
			end)
		end)
	end
	slider:New({
		Range={35,100},
		Increment=1,
		Suffix="/100%",
		Default=70,
		Callback=function(sliderudehealthplm)
			
			end
	})
	
end
coroutine.wrap(ZULY_fake_script)()
